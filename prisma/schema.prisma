model Content {
  id        String   @id @default(cuid())
  pageId    String   @unique
  title     String
  content   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Company {
  id          String   @id @default(cuid())
  name        String
  logo        String?
  address     String
  city        String
  state       String
  zipCode     String
  phone       String
  email       String
  password    String
  website     String?
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
